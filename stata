estimates store bottom_quartile_gincome6

/// lower quartile net income
xtreg revghq c.age##c.age pc1 pc2 educ_level labour_status ib(1).marital_status_group ib(1).race_cat bottom_quartile_nincome c.bottom_quartile_nincome#i.male, fe
estimates store bottom_quartile_nincome5
xtreg revghq c.age##c.age pc1 pc2 educ_level labour_status ib(1).marital_status_group ib(1).race_cat bottom_quartile_nincome c.bottom_quartile_nincome#i.male, vce(cluster pidp) fe
estimates store bottom_quartile_nincome6

/// Modelling for time index
gen t = mdy(istrtdaty, istrtdatm, istrtdatd)
format t %td

/// winsorised gross income
xtreg revghq pc1 pc2 educ_level labour_status ib(1).marital_status_group ib(1).race_cat winsor_gincome c.winsor_gincome#i.male t


